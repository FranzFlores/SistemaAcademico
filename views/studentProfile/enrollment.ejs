<%-include('../header')%>

<h3 class="center-align">Carrera de <%= cycles[0].curriculum.career.name%></h3>
<ul class="collapsible">
    <% cycles.forEach(function(cycle){  %>
    <li>
        <div class="collapsible-header"><%=cycle.cycle.name%> Ciclo</div>
        <div class="collapsible-body">
            <ul class="collection">
                <% cycle.subjects.forEach(function(subject){ %>
                    <a href="#!" class="collection-item"><%= subject.name%></a>
                <%})%>
            </ul>
        </div>
    </li>
       
    <%});%>
</ul>

<%-include('../footer')%>